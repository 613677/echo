void shortestpath(int v1,int v2,AdjMWGraph *G,int D[],int n,int s[])//计算v1到v2的最短路径

　　{
　　	int i,j;
　　	for(i=0;i<n;i++)
　　	{
　　		s[i]=0;
　　		D[i]=G->edge[v1][i];
　　	}
　　	s[v1]=1;
　　	D[v1]=0;
　　	for(i=0;i<n-1;i++)
　　	{
　　		for(j=0;j<n-1;j++)
　　		{
　　			s[j]=1;
　　			if(D[j]+G->edge[j][v2]<D[v2])
				D[v2]=D[j]+G->edge[j][v2];//更新最短路径
　　
　　		}
　　	}
　　	printf("%2d\n",D[v2]);
　　}
　　
